[
    {
        "batch_size": 10000,
        "batchable": "batchables/nlp/ngrammer",
        "env_files": [
            "/nesta/nesta",
            "/config/mysqldb.config"
        ],
        "job_def": "py36_amzn1_image",
        "job_name": "ngram",
        "job_queue": "HighPriority",
        "memory": 4000,
        "poll_time": 10,
        "region_name": "eu-west-2"
    },
    {
        "batchable": "batchables/nlp/tfidf",
        "extra": {
            "lower_tfidf_percentile": 5,
            "upper_tfidf_percentile": 90
        },
        "job_name": "tfidf",
        "memory": 16000,
        "n_batches": 1
    },
    {
        "batchable": "batchables/nlp/topic_model",
        "job_name": "topic_model",
        "memory": 4000
    },
    {
        "batchable": "batchables/nlp/synonyms",
        "child": "tfidf",
        "job_name": "synonyms",
        "memory": 4000
    },
    {
        "batchable": "batchables/nlp/ngram_analyser",
        "child": "ngram",
        "job_name": "ngram_analyser",
        "use_intermediate_inputs": true
    }
]